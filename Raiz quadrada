#include <stdio.h>
#include <math.h>

int main()
{   
    int numero, resultado;
    
    printf("Digite o número desejado para o calculo da raiz quadrada: ");
    scanf("%d", &numero);
    
    if (numero > 0){
        resultado = sqrt(numero); 
        printf("A raiz quadrada de %d, é de %d", numero, resultado);
    }
    else {
        printf("Número inválido!");
    }
    
    return 0;
}
